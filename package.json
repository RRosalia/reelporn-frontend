    "translate:sync": "node scripts/sync-translations.js",
    "translate:validate": "node scripts/validate-translations.js"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "next": "beta",
    "axios": "^1.12.2",
    "next-intl": "^4.3.12",
    "@laravel/echo-react": "^2.2.4",
    "laravel-echo": "^2.2.4",
