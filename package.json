    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-gtm-module": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^15.5.0",
    "deepl-node": "^1.20.0",
    "eslint": "^9",
    "eslint-config-next": "beta",
    "istanbul-lib-coverage": "^3.2.2",
